{
  "name": "docs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "docs/docs",
  "tags": ["type:app", "scope:docs"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/.rspress/dist"],
      "options": {
        "command": "rspress build",
        "cwd": "docs"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rspress dev",
        "cwd": "docs"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rspress preview",
        "cwd": "docs"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["docs/**/*.{ts,tsx,md}"]
      }
    }
  }
}
