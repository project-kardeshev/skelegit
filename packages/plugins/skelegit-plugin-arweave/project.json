{
  "name": "plugin-arweave",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/plugins/skelegit-plugin-arweave/src",
  "tags": ["type:lib", "scope:plugin"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/plugins/skelegit-plugin-arweave/dist"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/packages/plugins/skelegit-plugin-arweave"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/plugins/skelegit-plugin-arweave/**/*.ts"]
      }
    }
  }
}
