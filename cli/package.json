{
  "name": "@skelegit/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Skelegit",
  "main": "dist/index.js",
  "bin": {
    "skelegit": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@skelegit/core": "workspace:*",
    "effect": "^2.4.0",
    "ink": "^4.4.1",
    "react": "^18.2.0",
    "isomorphic-git": "^1.25.6",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.56",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitest": "^1.3.0"
  }
}
